<UserControl x:Class="TaekwondoScore.ListPages.WinBlue"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:TaekwondoScore.ListPages"
             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <Grid Background="Blue">

        <Button Style="{StaticResource tablobluebtn}" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20" Background="{x:Null}" BorderBrush="{x:Null}" Width="80" Height="60" Foreground="White" Cursor="Hand" x:Name="previousBtn"  Click="previousBtn_Click">
            <StackPanel Orientation="Horizontal">
                <md:PackIcon Width="80" Height="60" Kind="ArrowLeftBold"/>
            </StackPanel>
        </Button>
        <Label HorizontalAlignment="Center"  VerticalAlignment="Top" Content="Winner" FontSize="100" FontWeight="Bold" FontStyle="Italic" />
        <Label x:Name="winNameBluelbl" HorizontalAlignment="Center"  VerticalAlignment="Center" Content="CHON" FontSize="80" FontWeight="DemiBold"/>

    </Grid>
</UserControl>
